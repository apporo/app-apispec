{
  "name": "app-apispec",
  "version": "0.2.2",
  "description": "Devebot API specification layerware",
  "homepage": "https://github.com/apporo/app-apispec.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
    "prepublish": "NODE_ENV=production npm run build",
    "pretest": "NODE_ENV=test npm run build",
    "test": "cucumber-js ./test/**/*.feature",
    "verify": "npm run prepublish && cucumber-js ./test/**/*.feature"
  },
  "engines": {
    "node": "0.10 || 0.11 || 0.12 || 4"
  },
  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "app-webserver": "^0.2.2",
    "app-webweaver": "^0.2.2",
    "devebot": "^0.2.6"
  },
  "dependencies": {
    "app-webserver": "^0.2.2",
    "app-webweaver": "^0.2.2",
    "devebot": "^0.2.6",
    "swagger-tools": "0.10.1",
    "syncblock": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-mangle-names": "^0.4.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "cucumber": "^1.3.1"
  }
}
